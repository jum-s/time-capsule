{

  "manifest_version": 2,
  "name": "Browse Periodically 2",
  "version": "1.0",
  "description": "__MSG_description__",
  "icons": {
    "48": "icons/browse-periodically-48.png",
    "92": "icons/browse-periodically-92.png"
  },
  "permissions": [
    "storage",
    "tabs"
  ],
  "browser_action": {
    "default_icon": {
      "32" : "icons/browse-periodically-32.png"
    },
    "default_title": "Browse Periodically 2",
    "default_popup": "popup/popup.html",
    "browser_style": true
  },
  "background": {
    "scripts": ["background/background.build.js"]
  },
  "default_locale": "en"
}
